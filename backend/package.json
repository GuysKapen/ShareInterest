  "bugs": {
    "url": "https://github.com/romandunets/node-express-skeleton/issues"
  },
  "homepage": "https://github.com/romandunets/node-express-skeleton#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "config": "^3.3.7",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "google-auth-library": "^8.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.1",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
